#
# Enable saving performance monitoring data in the [system-dir]\diagnostics
# directory.
#
ide.savediagnostics=true

#
# Enable logging of excessively long pauses on the AWT 
# event dispach thread.
#
ide.awthangmonitor=true
ide.awthangmonitor.threshold=5000
ide.awthangmonitor.autologfiles=true
ide.awthangmonitor.allthreads=true
