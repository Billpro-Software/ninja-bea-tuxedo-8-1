TUXDIR=<full path for TUXDIR>; export TUXDIR
PATH=:$TUXDIR/bin:$PATH
APPDIR=<full path for APPDIR>; export APPDIR
TUXCONFIG=$APPDIR/tuxconfig; export TUXCONFIG
FIELDTBLS=Usysflds,bank.flds; export FIELDTBLS
FLDTBLDIR=$TUXDIR/udataobj:$APPDIR; export FLDTBLDIR
TDMCONFIG=$APPDIR/TDMCONFIG; export TDMCONFIG
LD_LIBRARY_PATH=$TUXDIR/lib; export LD_LIBRARY_PATH
SHLIB_PATH=$TUXDIR/lib; export SHLIB_PATH
LIBPATH=$TUXDIR/lib; export LIBPATH

