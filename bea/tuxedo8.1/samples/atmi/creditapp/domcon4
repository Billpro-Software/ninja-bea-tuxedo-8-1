	TYPE=TDOMAIN
	DOMAINID="XDOM3"
	BLOCKTIME=10   
	MAXDATALEN=56  
	MAXRDOM=89         
	DMTLOGDEV="<APPDIR2>/DMTLOG" 
	AUDITLOG="<APPDIR2>/AUDITLOG"
	DMTLOGNAME="DMTLOG_XDOM3"    



