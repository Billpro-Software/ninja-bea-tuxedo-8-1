	TYPE=TDOMAIN
	DOMAINID="TDOM1"
	BLOCKTIME=10   
	MAXDATALEN=56  
	MAXRDOM=89         
	DMTLOGDEV="<APPDIR2>/DMTLOG" 
	AUDITLOG="<APPDIR2>/AUDITLOG"
	DMTLOGNAME="DMTLOG_TDOM1"    

