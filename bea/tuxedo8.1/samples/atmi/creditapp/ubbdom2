*GROUPS
DEFAULT:	LMID=SITE2
BANKB1		GRPNO=1
BANKB3		GRPNO=3
BANKB2		GRPNO=2 TMSNAME=TMS_SQL	TMSCOUNT=2 
	OPENINFO="TUXEDO/SQL:<APPDIR2>/crdtdl2:bankdb:readwrite"
#
#
*SERVERS
#
DEFAULT: RESTART=Y MAXGEN=5 REPLYQ=Y CLOPT="-A"
DMADM 		SRVGRP=BANKB1	SRVID=32
GWADM 		SRVGRP=BANKB1	SRVID=30
GWTDOMAIN	SRVGRP=BANKB1	SRVID=31
GWADM 		SRVGRP=BANKB3	SRVID=40
GWTDOMAIN	SRVGRP=BANKB3	SRVID=41
TLR	SRVGRP=BANKB2	SRVID=3		RQADDR=tlr2	CLOPT="-A -- -T 600"
TLRA	SRVGRP=BANKB2	SRVID=5		CLOPT="-A -- -T 600"
XFER	SRVGRP=BANKB2	SRVID=6		
ACCT	SRVGRP=BANKB2	SRVID=7		
ACCTA	SRVGRP=BANKB2	SRVID=9
CRDT 	SRVGRP=BANKB2	SRVID=15
BTADD	SRVGRP=BANKB2	SRVID=12
#
*SERVICES
DEFAULT:	LOAD=50		AUTOTRAN=N
WITHDRAWALA	PRIO=50		ROUTING=C_ACCOUNT_ID
INQUIRYA   	PRIO=50		ROUTING=C_ACCOUNT_ID
OPENCA		PRIO=40		ROUTING=LAST_NAME   	
CLOSECA		PRIO=40		ROUTING=C_ACCOUNT_ID
DEPOSITA	PRIO=50	
OPEN_ACCT2	PRIO=40		
OPENC		PRIO=40		
BR_ADD		PRIO=20	
TLR_ADD		PRIO=20
WITHDRAWAL	PRIO=50		ROUTING=ACCOUNT_ID
DEPOSIT		PRIO=50		ROUTING=ACCOUNT_ID
TRANSFER	PRIO=50		ROUTING=ACCOUNT_ID
INQUIRY		PRIO=50		ROUTING=ACCOUNT_ID
CLOSE_ACCT	PRIO=40		ROUTING=ACCOUNT_ID
OPEN_ACCT	PRIO=40		ROUTING=BRANCH_ID
*ROUTING
C_ACCOUNT_ID	FIELD=C_ACCOUNT_ID
		BUFTYPE="FML"
		RANGES="*:BANKB1"
LAST_NAME   	FIELD=LAST_NAME   
		BUFTYPE="FML"
		RANGES="*:BANKB1"
ACCOUNT_ID	FIELD=ACCOUNT_ID
		BUFTYPE="FML"
		RANGES="10000-39999:BANKB2,
			40000-109999:BANKB3,
			*:*"
BRANCH_ID	FIELD=BRANCH_ID
		BUFTYPE="FML"
		RANGES="1-3:BANKB2,
			4-10:BANKB3,
			*:*"

